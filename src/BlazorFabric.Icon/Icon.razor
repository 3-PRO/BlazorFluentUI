    </div>
}
else
{
    <i class=@($"ms-Icon ms-Icon--{IconName} {(this.IsPlaceholder ? "ms-Icon-placeHolder" : "")} {this.ClassName}") role="presentation" @ref="RootElementRef">

    </i>
}


